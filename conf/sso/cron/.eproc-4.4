#
# Preferences pour les calculs d'ephemerides
#
ephem.numplanet          = 4            # Mars
ephem.cata               = HIP          # catalogue d'etoile
ephem.teph               = 1            # 1:astrom. J2000, 2:apparent, 3:moyen de la date, 4:geometrique
ephem.pref               = 1            # 1:equateur 2:ecliptique
ephem.trep               = 1            # 1:spheriques, 2:rectang., 3:locales, 4:horaires,  5:dediees obs.
ephem.keph               = 7            # 1:De200, 2:Bdl82, 3:De403, 4:Vsop87, 5:DE405, 6:DE406, 7:INPOP, 8:DE430, 9:DE431
ephem.theomod            = 0            # 0:Par defaut 2,3,4,5:Integration numerique
ephem.cent               = 2            # 1:helio. 2:geoc. 3:topoc. 4:sonde
ephem.elosc              = 1            # 1:Bowell, 2: pos./vit. init., 3:utilisateur
ephem.termrela           = 1            # (0,1) prise en compte ou non de la relativite
ephem.termalpha          = 0            # (0,1,2) choix du syst. de coord. relativiste
ephem.echel              = TT           # TT ou UTC
ephem.ndat               = 5            # nombre de date de calcul (< 5000)
ephem.pas                = 1.0          # pas de calcul en jour et fraction de jour
ephem.fmt_date           = greg         # format d'affichage par defaut des dates (greg|iso|jd)
ephem.date_file_fmt      = 1            # format du fichier dates utilisateur
#
# Station d'observation
#
ephem.lieu               = Observatoire de Paris (WGS84)
ephem.geo1               =  2.20114874  # longitude en deg.mmsssss (negative vers l'ouest)
ephem.geo2               = 48.50113200  # latitude en deg.mmsssss (negative vers le sud)
ephem.geo3               = 67.0         # altitude en metres
#
# Preferences pour la reduction astrometrique
#
reduction.format         = Priam       # format du fichier de mesure
reduction.mode           = simple      # mode de reduction: simple, avance, expert
